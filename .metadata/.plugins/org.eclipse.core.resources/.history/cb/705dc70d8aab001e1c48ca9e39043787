package overload;

public class Cat {
	public String name;
	public int age;
	
	// 생성자 오버로딩은 생성자를 여러 유형으로 만드는 것
	// 파라미터는 () ->void 파라미터
	// 				(String, int)
	// 두가지 유형으로 만들게요
	public Call() {
		name = "XX아 이리와";
	}
	
	public Call(String n) { // String, int 파라미터, new Human("문자" , 정수)인 경우 호출
		name = n;
	}
}
